{
  "name": "forms",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build": "react-scripts build",
    "postbuild": "buildinfo build/build-info.txt",
    "e2e": "cross-env PORT=4321 concurrently --kill-others --success first \"npm:e2e:*\"",
    "pree2e:app": "npm run build",
    "e2e:app": "serve -p $PORT build/",
    "pree2e:run": "wait-on --log --timeout 30000 http-get://localhost:$PORT",
    "e2e:run": "cross-env CYPRESS_BASE_URL=http://localhost:$PORT cypress run",
    "eject": "react-scripts eject",
    "lint": "eslint --max-warnings 0 .",
    "ship": "npm run lint && npm run test -- --watchAll false && npm run e2e",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "@codeyourfuture/standard",
      "react-app",
      "react-app/jest"
    ],
    "ignorePatterns": [
      "build/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@codeyourfuture/eslint-config-standard": "^3.0.3",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@textbook/build-info": "^0.2.3",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^10.3.0",
    "eslint-config-cypress": "^0.28.0",
    "react-scripts": "5.0.1",
    "serve": "^14.0.0",
    "wait-on": "^6.0.1"
  }
}
